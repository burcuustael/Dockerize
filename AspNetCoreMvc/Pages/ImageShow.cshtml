@page
@model AspNetCoreMvc.Pages.ImageShowModel
@{
}

<h1>Resimler</h1>

@foreach (var image in Model.Images)
{
    <div>
    <img src="~/images/@image" style="width:100px; height:100px;border:1px solid red;" />
    <h3>@image</h3>
    </div>
}

<form method="post" asp-action="ImageShow">
    <input type="text" asp-="name" />
    <input type="submit" value="sil" />
</form>